ct}}" data-event-opts="{{[['^confirm',[['getChooseRegionVal']]]]}}" bind:confirm="__e" bind:__l="__l"></choose-region><input class="hidden" name="region" value="{{regionName}}"/><input class="hidden" name="region_id" value="{{regionId}}"/><input class="hidden" name="region_name" value="{{regionName}}"/><view class="form-group"><view class="title">详细地址</view><input placeholder="请输入详细地址" name="details_address" value="{{data.more.details_address}}"/></view><view class="form-group"><view class="title">单位名称</view><input placeholder="请输入单位名称" name="company_name" value="{{data.company_name}}"/></view><view class="form-group"><view class="title">专业工种</view><input placeholder="请选择专业工种" name="work" disabled="true" data-event-opts="{{[['tap',[['togglePopup',['open','cate_id']]]]]}}" value="{{cateName}}" bindtap="__e"/><input class="hidden" name="cate_id" value="{{cateId}}"/><input class="hidden" name="cate_name" value="{{cateName}}"/><uni-popup class="vue-ref" vue-id="c266ed70-2" custom="{{true}}" data-ref="cate_id" bind:__l="__l" vue-slots="{{['default']}}"><popup-cate vue-id="{{('c266ed70-3')+','+('c266ed70-2')}}" headerTitle="请选择专业工种" cateType="zhaohuo" overstepLengthTips="专业工种" chooseLength="5" defaultSelectedVal="{{cateId}}" defaultSelectedName="{{cateName}}" data-event-opts="{{[['^close',[['togglePopup']]],['^click',[['confirmCateChoose']]]]}}" bind:close="__e" bind:click="__e" bind:__l="__l"></popup-cate></uni-popup></view><view class="form-group"><input class="hidden" name="id_card_img" value="null"/><input class="hidden" name="hand_id_card_img" value="null"/><input class="hidden" name="user_id" value="{{userInfo.uid}}"/><block wx:if="{{data.id}}"><input class="hidden" name="id" value="{{data.id}}"/></block><button class="submit-btn" type="default" formType="submit">立即提交</button></view></form></block></view>