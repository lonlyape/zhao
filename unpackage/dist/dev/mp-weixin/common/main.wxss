/* 表单 */
.form-group {
  background-color: #ffffff;
  padding: 1rpx 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  min-height: 100rpx;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  border-top: 1rpx solid #ededed;
}
.form-group .title {
  text-align: justify;
  padding-right: 30rpx;
  font-size: 28rpx;
  position: relative;
  height: 60rpx;
  line-height: 60rpx;
  min-width: calc(4em + 15px);
}
.form-group input {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  font-size: 30rpx;
  color: #555;
  padding-right: 20rpx;
}
.form-group textarea {
  margin: 32rpx 0 30rpx;
  height: 4.6em;
  width: 100%;
  line-height: 1.2em;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  font-size: 28rpx;
  padding: 0;
}
.form-group .view-textarea {
  margin: 32rpx 0 30rpx;
  height: 4.6em;
  width: 100%;
  line-height: 1.2em;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  font-size: 28rpx;
  padding: 0;
}
.form-group picker {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  padding-right: 40rpx;
  overflow: hidden;
  position: relative;
}
.form-group picker .picker {
  font-size: 28rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  width: 100%;
  /* text-align: right; */
}
.form-group .submit-btn {
  display: block;
  width: 70%;
  margin: 20rpx auto;
  background: #0099ff;
  color: #ffffff;
}
.form-button_box {
  padding: 60rpx 0;
  background-color: rgba(255, 255, 255, 0);
  border: none;
}
.form-button_box .submit-btn {
  border-radius: 60rpx;
  background: -webkit-linear-gradient(left, #015fea, #1995fb);
  background: linear-gradient(to right, #015fea, #1995fb);
}
